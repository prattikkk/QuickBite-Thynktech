# ── QuickBite production env ──────────────────────────────────
# Copy to .env.prod and fill in real values; NEVER commit this file
# ──────────────────────────────────────────────────────────────

# Database
DB_USERNAME=dbuser
DB_PASSWORD=CHANGE_ME_STRONG_PASSWORD
DB_NAME=quickbite

# JWT
JWT_SECRET=CHANGE_ME_MINIMUM_256_BIT_RANDOM_SECRET

# Payment provider
PAYMENT_PROVIDER=generic
PAYMENT_API_KEY=
PAYMENT_API_SECRET=
PAYMENT_WEBHOOK_SECRET=CHANGE_ME_WEBHOOK_SECRET

# CORS
CORS_ORIGINS=https://yourdomain.com

# Image registry
REGISTRY=ghcr.io
REPO=your-org/quickbite
TAG=latest

# Ports (override if needed)
BACKEND_PORT=8080
FRONTEND_PORT=80
