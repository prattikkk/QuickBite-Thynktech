# QuickBite Frontend Environment Variables

# Backend API Base URL (must include /api path)
VITE_API_BASE_URL=http://localhost:8080/api

# Stripe Publishable Key (pk_test_... or pk_live_...)
VITE_STRIPE_PUBLISHABLE_KEY=

# WebSocket URL (for order updates)
VITE_WS_URL=ws://localhost:8080/ws

# Enable WebSocket (set to 'true' to use WebSocket instead of polling)
VITE_USE_WEBSOCKET=false

# Order status polling interval (milliseconds)
VITE_POLL_INTERVAL=5000

# Mapbox GL JS token for live maps & delivery radius preview
VITE_MAPBOX_TOKEN=
